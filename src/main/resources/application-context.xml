<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:annotation-config />

	<bean id ="jefe1" class="com.curso.Practica_Spring3.domain.JefeEquipo"></bean>
	<bean id ="programador1" class="com.curso.Practica_Spring3.domain.Programador">
		<qualifier value="main" />
	</bean>
	
	<bean id ="programador2" class="com.curso.Practica_Spring3.domain.Programador">
		<property name="tarea" value="Programar en java" />
	</bean>
	
	
	
	
</beans>
